{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts",
      "methods": ["OPTIONS", "GET", "POST"], 
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
